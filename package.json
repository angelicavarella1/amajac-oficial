{
  "name": "amajac-oficial",
  "type": "module",
  "version": "1.0.0",
  "description": "Projeto AMAJAC com verificações de segurança",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "security:scan": "node security-scanner.mjs",
    "security:lint": "node security-lint.mjs",
    "security:smart": "node security-lint-smart.mjs",
    "security:fix": "node apply-security-fixes.mjs",
    "security:all": "npm run security:fix && npm run security:lint && npm run security:scan",
    "security:report": "npm run security:smart",
    "pre-commit": "npm run security:smart",
    "lint": "eslint \"{src,composables,utils}/**/*.{js,vue,ts}\" --ignore-pattern \"**/node_modules/**\" --ignore-pattern \"**/dist/**\" --ignore-pattern \"**/.nuxt/**\" --ignore-pattern \"**/functions-serve/**\" --ext .js,.vue,.ts --fix",
    "lint:security": "eslint \"{src,composables,utils}/**/*.{js,vue,ts}\" --ignore-pattern \"**/node_modules/**\" --ignore-pattern \"**/dist/**\" --ignore-pattern \"**/.nuxt/**\" --ignore-pattern \"**/functions-serve/**\" --config .eslintrc.security.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@supabase/supabase-js": "^2.58.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.1",
    "jspdf": "^3.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript-eslint": "^8.46.2",
    "vite": "^4.0.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "keywords": [
    "vue",
    "security",
    "supabase",
    "tailwindcss"
  ],
  "author": "AMAJAC",
  "license": "MIT"
}
